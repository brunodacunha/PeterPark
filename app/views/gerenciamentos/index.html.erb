<h1>Listing gerenciamentos</h1>

<table>
  <thead>
    <tr>
      <th>Valor hora</th>
      <th>Horaadd</th>
      <th>Valor diaria</th>
      <th>Mensalista meio turno</th>
      <th>Mensalista integral</th>
      <th>Num vagas</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @gerenciamentos.each do |gerenciamento| %>
      <tr>
        <td><%= gerenciamento.valor_hora %></td>
        <td><%= gerenciamento.horaadd %></td>
        <td><%= gerenciamento.valor_diaria %></td>
        <td><%= gerenciamento.mensalista_meio_turno %></td>
        <td><%= gerenciamento.mensalista_integral %></td>
        <td><%= gerenciamento.num_vagas %></td>
        <td><%= link_to 'Show', gerenciamento %></td>
        <td><%= link_to 'Edit', edit_gerenciamento_path(gerenciamento) %></td>
        <td><%= link_to 'Destroy', gerenciamento, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Gerenciamento', new_gerenciamento_path %>
